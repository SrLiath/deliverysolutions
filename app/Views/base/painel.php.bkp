                          `;

                      var btnUser = document.getElementById('desativarBtn');
                      btnUser.setAttribute('onclick', `openModale('${userId}', '5')`);

                      // Abrir o modal
                      var userDataModal = new bootstrap.Modal(document.getElementById('userDataModal_e'));
                      userDataModal.show();
